class Solution {
    public int[] plusOne(int[] D) {
       int n=D.length;
        for(int i=n-1; i>=0; i--){
            if(D[i]<9){
                D[i]++;
                return D;
            }else{
                D[i]=0;
            }
        }
        int a[]=new int[n+1];
        a[0]=1;
        return a;
        }
        }
